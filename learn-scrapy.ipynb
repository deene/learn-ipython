     "input": [
      "from scrapy import Spider, Item, Field\n",
      "\n",
      "class Post(Item):\n",
      "    title = Field()\n",
      "\n",
      "class BlogSpider(Spider):\n",
      "    name, start_urls = 'blogspider', ['http://blog.scrapinghub.com']\n",
      "\n",
      "    def parse(self, response):\n",
      "        return [Post(title=e.extract()) for e in response.css(\"h2 a::text\")]"
     ],
     "language": "python",
