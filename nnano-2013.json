			"prism:productCode": "nnano",
			"dc:creator": 
					[
					                    "Tae-Gon Cha"
							,
						
                    "Jing Pan"
							,
						
                    "Haorong Chen"
							,
						
                    "Janette Salgado"
							,
						
                    "Xiang Li"
