			"dc:title": "Control of single-spin magnetic anisotropy by exchange coupling",
			"prism:productCode": "nnano",
			"dc:creator": 
					[
					                    "Jenny C. Oberg"
							,
						
                    "M. Reyes Calvo"
							,
						
                    "Fernando Delgado"
							,
						
                    "María Moro-Lagares"
