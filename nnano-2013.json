			"prism:productCode": "nnano",
			"dc:creator": 
					[
					                    "Michael Geiselmann"
							,
						
                    "Mathieu L. Juan"
							,
						
                    "Jan Renger"
							,
						
                    "Jana M. Say"
							,
						
                    "Louise J. Brown"
