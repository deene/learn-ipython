			"dc:title": "Tracking the engraftment and regenerative capabilities of transplanted lung stem cells using fluorescent nanodiamonds",
			"prism:productCode": "nnano",
			"dc:creator": 
					[
					                    "Tsai-Jung Wu"
							,
						
                    "Yan-Kai Tzeng"
							,
						
                    "Wei-Wei Chang"
							,
						
                    "Chi-An Cheng"
							,
						
                    "Yung Kuo"
							,
						
                    "Chin-Hsiang Chien"
