			"dc:creator": 
					[
					                    "V. S. Pribiag"
							,
						
                    "S. Nadj-Perge"
							,
						
                    "S. M. Frolov"
							,
						
                    "J. W. G. van den Berg"
							,
						
                    "I. van Weperen"
							,
						
                    "S. R. Plissard"
							,
						
                    "E. P. A. M. Bakkers"
