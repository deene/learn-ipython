			"dc:identifier": "doi:10.1038/nnano.2013.265",
			"dc:title": "Propagation of magnetic vortices using nanocontacts as tunable attractors",
			"prism:productCode": "nnano",
			"dc:creator": 
					[
					                    "M. Manfrini"
							,
						
                    "Joo-Von Kim"
							,
						
                    "S. Petit-Watelot"
							,
						
                    "W. Van Roy"
							,
						
                    "L. Lagae"
